<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="TestNGListenerSuite">
    <listeners>
        <listener class-name="Listeners"/>
    </listeners>
    <test name="LoginTest">
        <classes>
            <class name="ListenerLoginTest">
                <methods>
                    <include name="loginByEmail"/>
                    <exclude name="loginByFacebook"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>